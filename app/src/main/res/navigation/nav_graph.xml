<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/notes_fragment">

    <fragment
        android:id="@+id/notes_fragment"
        android:name="ru.kpfu.itis.springpractice.experiment.presentation.ui.fragment.ExperimentFragment"
        android:label="fragment_experiment"
        tools:layout="@layout/fragment_experiment" >
        <action
            android:id="@+id/action_notes_fragment_to_note_adding_fragment"
            app:destination="@id/note_adding_fragment" />
    </fragment>
    <fragment
        android:id="@+id/map_fragment"
        android:name="ru.kpfu.itis.springpractice.experiment.presentation.ui.fragment.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
    </fragment>
    <fragment
        android:id="@+id/profile_fragment"
        android:name="ru.kpfu.itis.springpractice.experiment.presentation.ui.fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
    </fragment>
    <fragment
        android:id="@+id/note_adding_fragment"
        android:name="ru.kpfu.itis.springpractice.experiment.presentation.ui.fragment.NoteAddingFragment"
        android:label="fragment_note_adding"
        tools:layout="@layout/fragment_note_adding" >
    </fragment>
</navigation>